{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "lib": ["es2023"],
    "module": "node16",
    "moduleDetection": "force",
    "moduleResolution": "node16",
    "noEmit": true,
    "target": "es2022"
  },
  "extends": "./tsconfig.json",
  "include": ["vite.config.ts"]
}
